{
  "name": "nuxt3-project",
  "version": "1.0.0",
  "description": "My Nuxt.js Element Admin project based by @panjiachen vue-element-admin",
  "author": "bikai",
  "private": true,
  "main": "api-server/index.js",
  "keywords": [],
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": 3010
    }
  },
  "scripts": {
    "dev": "nuxt",
    "dev-debug": "node --inspect-brk ./node_modules/nuxt/bin/nuxt dev --port 3021",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.5",
    "@nuxtjs/proxy": "^2.1.0",
    "bluebird": "^3.7.2",
    "co": "^4.6.0",
    "core-js": "^3.8.3",
    "convert-excel-to-json": "^1.7.0",
    "cross-env": "^5.2.0",
    "del": "^6.0.0",
    "element-ui": "^2.15.0",
    "file-type": "^16.2.0",
    "fs-extra": "^9.1.0",
    "is-absolute-url": "^3.0.3",
    "is-there": "^4.5.1",
    "js-cookie": "^2.2.1",
    "js-yaml": "^4.0.0",
    "jsonfile": "^6.1.0",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-bodyparser": "^4.3.0",
    "koa-log4": "^2.3.2",
    "koa-redis": "^4.0.1",
    "koa-router": "^10.0.0",
    "koa-session": "^6.1.0",
    "koa-static": "^5.0.0",
    "ldapjs": "^2.2.3",
    "lodash": "^4.17.20",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.1",
    "mongoose": "^5.11.13",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.14.12",
    "shelljs": "^0.8.4",
    "statuses": "^2.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@vue/test-utils": "^1.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.5.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "node-sass": "^5.0.0",
    "nodemon": "^1.18.9",
    "prettier": "^2.2.1",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.9.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "vue-jest": "^3.0.4"
  }
}
